package org.gol.ide;

import com.google.inject.Guice;
import com.google.inject.Injector;
import org.eclipse.xtext.util.Modules2;
import org.gol.GolRuntimeModule;
import org.gol.GolStandaloneSetup;

/**
 * Initialization support for running Xtext languages as language servers.
 */
public class GolIdeSetup extends GolStandaloneSetup {

	@Override
	public Injector createInjector() {
		return Guice.createInjector(Modules2.mixin(new GolRuntimeModule(), new GolIdeModule()));
	}
	
}
